<?xml version="1.0" encoding="UTF-8"?>
<project name="module_hongbao-task" default="compile.module.hongbao-task">
  <dirname property="module.hongbao-task.basedir" file="${ant.file.module_hongbao-task}"/>
  
  <property name="module.jdk.home.hongbao-task" value=""/>
  <property name="module.jdk.bin.hongbao-task" value=""/>
  <property name="module.jdk.classpath.hongbao-task" value=""/>
  
  <property name="compiler.args.hongbao-task" value="-encoding UTF-8 -source 8 -target 1.7 ${compiler.args}"/>
  
  <property name="hongbao-task.output.dir" value="undefined"/>
  <property name="hongbao-task.testoutput.dir" value="undefined"/>
  
  <path id="hongbao-task.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="hongbao-task.module.production.classpath"/>
  
  <path id="hongbao-task.runtime.production.module.classpath"/>
  
  <path id="hongbao-task.module.classpath"/>
  
  <path id="hongbao-task.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.hongbao-task">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.hongbao-task">
    <patternset refid="excluded.from.module.hongbao-task"/>
  </patternset>
  
  
  <target name="compile.module.hongbao-task" depends="compile.module.hongbao-task.production,compile.module.hongbao-task.tests" description="Compile module hongbao-task"/>
  
  <target name="compile.module.hongbao-task.production" depends="register.custom.compilers" description="Compile module hongbao-task; production classes"/>
  
  <target name="compile.module.hongbao-task.tests" depends="register.custom.compilers,compile.module.hongbao-task.production" description="compile module hongbao-task; test classes" unless="skip.tests"/>
  
  <target name="clean.module.hongbao-task" description="cleanup module">
    <delete dir="${hongbao-task.output.dir}"/>
    <delete dir="${hongbao-task.testoutput.dir}"/>
  </target>
</project>