<?xml version="1.0" encoding="UTF-8"?>
<project name="module_hongbao-model" default="compile.module.hongbao-model">
  <dirname property="module.hongbao-model.basedir" file="${ant.file.module_hongbao-model}"/>
  
  <property name="module.jdk.home.hongbao-model" value=""/>
  <property name="module.jdk.bin.hongbao-model" value=""/>
  <property name="module.jdk.classpath.hongbao-model" value=""/>
  
  <property name="compiler.args.hongbao-model" value="-encoding UTF-8 -source 8 -target 1.7 ${compiler.args}"/>
  
  <property name="hongbao-model.output.dir" value="undefined"/>
  <property name="hongbao-model.testoutput.dir" value="undefined"/>
  
  <path id="hongbao-model.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="hongbao-model.module.production.classpath"/>
  
  <path id="hongbao-model.runtime.production.module.classpath"/>
  
  <path id="hongbao-model.module.classpath"/>
  
  <path id="hongbao-model.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.hongbao-model">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.hongbao-model">
    <patternset refid="excluded.from.module.hongbao-model"/>
  </patternset>
  
  
  <target name="compile.module.hongbao-model" depends="compile.module.hongbao-model.production,compile.module.hongbao-model.tests" description="Compile module hongbao-model"/>
  
  <target name="compile.module.hongbao-model.production" depends="register.custom.compilers" description="Compile module hongbao-model; production classes"/>
  
  <target name="compile.module.hongbao-model.tests" depends="register.custom.compilers,compile.module.hongbao-model.production" description="compile module hongbao-model; test classes" unless="skip.tests"/>
  
  <target name="clean.module.hongbao-model" description="cleanup module">
    <delete dir="${hongbao-model.output.dir}"/>
    <delete dir="${hongbao-model.testoutput.dir}"/>
  </target>
</project>