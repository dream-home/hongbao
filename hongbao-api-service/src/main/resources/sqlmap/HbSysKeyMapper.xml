<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yanbao.mapper.HbSysKeyMapper">
	
	

	<!-- 获取首页广告列表 -->
	<insert id="insert">
		insert into hb_sys_key(id,issId) values(#{model.id},#{model.issId})
	</insert>
	
	<!-- 获取首页广告列表 -->
	<select id="delById">
		delete from hb_sys_key where id=#{model.id}
	</select>
</mapper>


